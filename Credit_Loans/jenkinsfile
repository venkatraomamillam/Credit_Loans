pipeline {
   agent any
   environment{
       PATH="/usr/local/src/apache-maven/bin:$PATH"
       }
   stages {
      stage('git clone') {
         steps {
            git credentialsId: 'venkat', url: 'https://github.com/venkatraomamillam/Project1.git'
         }
      }
     
     stage('change directory'){
     steps {
          sh "$PWD"
          dir('Credit_Loans'){
            sh "$PWD"
          }
          sh "$PWD"
        }
     } 
      
   stage('Maven Build') {
         steps {
            sh "mvn clean package"
         }
      }  
   }
}
