pipeline {
   agent any
   environment{
       PATH="/usr/local/src/apache-maven/bin:$PATH"
       }
   stages {
      stage('git clone') {
         steps {
            git credentialsId: 'venkat', url: 'https://github.com/venkatraomamillam/Credit_Loans.git'
         }
      }
    
     
      
   stage('Maven Build') {
         steps {
            sh "mvn clean package"
         }
      }  
   }
}
